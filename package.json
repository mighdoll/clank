{
  "name": "clank-cli",
  "version": "0.1.52",
  "description": "Keep AI files in a separate overlay repository",
  "type": "module",
  "bin": {
    "clank": "./bin/clank.ts"
  },
  "files": [
    "bin/",
    "src/"
  ],
  "scripts": {
    "test": "vitest",
    "test:ui": "vitest --ui",
    "typecheck": "tsc --noEmit",
    "bump": "monobump",
    "install": "pnpm install --global $PWD",
    "build": "tsc",
    "lint": "biome check",
    "fix": "biome check --fix --unsafe"
  },
  "engines": {
    "node": ">=22.6.0"
  },
  "keywords": [
    "ai",
    "assistant",
    "claude",
    "gemini",
    "copilot",
    "configuration",
    "worktree",
    "git"
  ],
  "author": "",
  "license": "MIT",
  "packageManager": "pnpm@10.23.0",
  "dependencies": {
    "commander": "^14.0.2",
    "cosmiconfig": "^9.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.8",
    "@types/node": "^24.10.1",
    "execa": "^9.6.1",
    "monobump": "^0.1.5",
    "typescript": "^5.9.3",
    "vitest": "^4.0.14"
  }
}
