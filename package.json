{
  "name": "clank-cli",
  "description": "Keep AI files in a separate overlay repository",
  "version": "0.1.54",
  "author": "",
  "type": "module",
  "bin": {
    "clank": "./bin/clank.ts"
  },
  "files": [
    "bin/",
    "src/"
  ],
  "repository": "github:mighdoll/clank",
  "scripts": {
    "bump": "monobump",
    "fix": "biome check --fix --unsafe",
    "fix:pkgJsonFormat": "syncpack format",
    "global": "pnpm link --global",
    "lint": "biome check --error-on-warnings",
    "prepublishOnly": "run-s lint typecheck fix:pkgJsonFormat test:once",
    "test": "vitest",
    "test:once": "vitest run",
    "test:ui": "vitest --ui",
    "typecheck": "tsgo"
  },
  "dependencies": {
    "commander": "^14.0.2",
    "cosmiconfig": "^9.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.8",
    "@types/node": "^24.10.1",
    "@typescript/native-preview": "7.0.0-dev.20251221.1",
    "@vitest/ui": "^4.0.16",
    "execa": "^9.6.1",
    "monobump": "^0.1.5",
    "npm-run-all2": "^7.0.2",
    "syncpack": "^13.0.4",
    "vitest": "^4.0.14"
  },
  "license": "MIT",
  "keywords": [
    "agent",
    "ai",
    "assistant",
    "claude",
    "configuration",
    "copilot",
    "gemini",
    "git",
    "worktree"
  ],
  "packageManager": "pnpm@10.23.0",
  "engines": {
    "node": ">=22.6.0"
  }
}
